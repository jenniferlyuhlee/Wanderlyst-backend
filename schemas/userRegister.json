{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "User Registration Schema",
    "required": [
        "username",
        "password",
        "email",
        "firstName",
        "lastName"
    ],
    "properties": {
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 25
        },
        "password": {
            "type": "string",
            "minLength": 5
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "firstName": {
            "type": "string",
            "minLength": 1
        },
        "lastName": {
            "type": "string",
            "minLength": 1
        }
    },
    "additionalProperties": false
}